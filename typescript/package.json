{
  "name": "@meet-expression/core",
  "version": "1.0.0",
  "description": "Face Expression Analysis Engine - TypeScript module for React",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "private": true,
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@mediapipe/tasks-vision": "^0.10.11",
    "typescript": "^5"
  },
  "peerDependenciesMeta": {
    "@mediapipe/tasks-vision": {
      "optional": false
    }
  },
  "keywords": [
    "face-expression",
    "mediapipe",
    "facs",
    "microexpressions",
    "emotion-detection"
  ]
}
